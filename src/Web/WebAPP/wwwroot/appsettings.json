{
  "ECommerceHttpClientOptions": {
    "CatalogEndpoint": "api/v1/catalogs",
    "OverallTimeout": "00:00:30",
    "RetryCount": 2,
    "SleepDurationPower": 2,
    "EachRetryTimeout": 5,
    "CircuitBreaking": 5,
    "DurationOfBreak": "00:00:10"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "BrowserConsole"
      }
    ]
  },
  "AllowedHosts": "*"
}
