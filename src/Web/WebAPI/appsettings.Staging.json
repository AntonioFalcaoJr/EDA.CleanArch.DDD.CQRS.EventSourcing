{
  "EventBusOptions": {
    "ConnectionString": "amqp://guest:guest@rabbitmq:5672/eventual-shop"
  },
  "AccountGrpcClientOptions": {
    "BaseAddress": "dns:///account-query"
  },
  "CatalogGrpcClientOptions": {
    "BaseAddress": "dns:///catalog-query"
  },
  "CommunicationGrpcClientOptions": {
    "BaseAddress": "dns:///communication-query"
  },
  "IdentityGrpcClientOptions": {
    "BaseAddress": "dns:///identity-query"
  },
  "OrderGrpcClientOptions": {
    "BaseAddress": "dns:///order-query"
  },
  "PaymentGrpcClientOptions": {
    "BaseAddress": "dns:///payment-query"
  },
  "ShoppingCartGrpcClientOptions": {
    "BaseAddress": "dns:///shopping-cart-query"
  },
  "WarehouseGrpcClientOptions": {
    "BaseAddress": "dns:///warehouse-query"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Elasticsearch"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "Formatter": "Serilog.Formatting.Elasticsearch.ElasticsearchJsonFormatter,Serilog.Formatting.Elasticsearch",
          "Theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}